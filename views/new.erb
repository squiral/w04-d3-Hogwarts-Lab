<form action="index.html" method="post">

<label for="first-name-field">First Name:</label>
<input type="text"
name="first_name"
id="first-name-field"
/>

<label for="last-name-field">Last Name:</label>
<input type="text"
name="last_name"
id="last-name-field"
/>

<label for="age-field">Age:</label>
<input type="number"
 name="age"
 id="age-field"
 />


<label for="house_select">Select House:</label>
<select name="house_id" id="house_select">
  <option value="1">Gryffindor</option>
  <option value="2">Hufflepuff</option>
  <option value="3">Ravenclaw</option>
  <option value="4">Slytherin</option>
</select>

<input type="submit" value="Enroll Student">

</form>
